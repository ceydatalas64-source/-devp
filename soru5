type kullanici = {
  id: number;
  name: string;
  email: string;
};

const users: kullanici[] = [
  { id: 1, name: "BAYRAM", email: "bayram@mail.com" },
  { id: 2, name: "AyÅŸe", email: "ayse@mail.com" },
  { id: 3, name: "BAYRAM", email: "bayram2@mail.com" },
];

// ğŸ”¹ Fonksiyon aÅŸÄ±rÄ± yÃ¼klemeleri
function search(id: number): kullanici;
function search(name: string): kullanici[];

// ğŸ”¹ Tek implementasyon
function search(param: number | string): kullanici | kullanici[] {
  if (typeof param === "number") {
    return users.find(u => u.id === param)!;
  } else {
    return users.filter(u => u.name === param);
  }
}

// ğŸ”¹ Ekran Ã§Ä±ktÄ±sÄ± (hocanÄ±n PasteCode baÄŸlantÄ±sÄ±ndakiyle aynÄ±)
console.log(search(1));
console.log(search("Ali"));
